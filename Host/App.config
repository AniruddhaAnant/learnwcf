<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <system.serviceModel>
    <services>
      <service name="ServicesLib.Calc">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9999"/>
            <add baseAddress="net.tcp://localhost:9998"/>
          </baseAddresses>
        </host>
        <endpoint address="calc" binding="basicHttpBinding" contract="ContractsLib.ICalc"/>
        <endpoint address="tcp/calc" binding="netTcpBinding" contract="ContractsLib.ICalc"/>
      </service>  
    </services>
  <behaviors>
    <serviceBehaviors>
      <behavior>
        <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true"/>
      </behavior>
    </serviceBehaviors>
  </behaviors>
  </system.serviceModel>
</configuration>